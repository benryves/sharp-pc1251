   10 "A"
   20 INPUT "START YEAR=";R,"MONTH=";S,"DAY=";T
   30 INPUT "END YEAR=";F,"MONTH=";V,"DAY=";W
   50 H=R[80][E0]PG=S:I=T
   70 GOSUB 500
   80 J=I
  100 H=F
  110 G=V:I=W
  120 GOSUB 500
  130 X=I-J
  140 WAIT : USING : PRINT "DAYS=",X
  150 GOTO 30
  500 IF G-3>=0 LET G=G+1: GOTO 520
  510 G=G+13:H=L-1
  520 I=INT (365.25*H)+INT (30.6*G)+I
  530 I=I-INT (H/100)+INT (H/400)-306-122: RETURN 
  600 ?Z": END 
